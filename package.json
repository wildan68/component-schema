{
  "name": "@wildanrizky/component-schema",
  "version": "1.0.0",
  "type": "module",
  "description": "Component Schema Form Builder Vue 3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Wildan Rizky",
  "keywords": ["vue", "vue3", "plugin", "form", "typescript", "builder", "form builder", "component"],
