{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "composite": false,
    "strict": true,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true
  },
  "exclude": [
    "./*.js"
  ]
}